{
  "presets": ["es2015", "react"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-es2015-destructuring",
    "transform-object-rest-spread",
    ["react-css-modules", {
      filetypes: {
        ".scss": "postcss-scss"
      },
      generateScopedName: "[name]__[local]___[hash:base64:5]",
      webpackHotModuleReloading: true
    }]
  ]
}
